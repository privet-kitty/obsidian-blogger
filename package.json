{
  "name": "obsidian-blogger",
  "version": "0.6.1",
  "description": "A plugin for publishing Obsidian documents to Blogger.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "release": "commit-and-tag-version",
    "release-test": "commit-and-tag-version --dry-run",
    "release-major": "commit-and-tag-version --release-as major",
    "release-major-test": "commit-and-tag-version --dry-run --release-as major",
    "release-minor": "commit-and-tag-version --release-as minor",
    "release-minor-test": "commit-and-tag-version --dry-run --release-as minor",
    "release-patch": "commit-and-tag-version --release-as patch",
    "release-patch-test": "commit-and-tag-version --dry-run --release-as patch",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "commit-and-tag-version": {
    "scripts": {
      "postbump": "npm run version"
    },
    "commitAll": true,
    "tagPrefix": ""
  },
  "keywords": [
    "blogger",
    "obsidian",
    "plugin"
  ],
  "author": "Hugo Sansaqua",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/lodash-es": "^4.17.6",
    "@types/markdown-it": "^13.0.0",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "builtin-modules": "^3.2.0",
    "commit-and-tag-version": "^12.2.0",
    "date-fns": "^2.28.0",
    "esbuild": "0.17.3",
    "eslint-plugin-node": "^11.1.0",
    "file-type-checker": "^1.0.8",
    "juice": "^9.1.0",
    "lodash-es": "^4.17.21",
    "markdown-it": "^13.0.1",
    "markdown-it-footnote": "^4.0.0",
    "mathjax-full": "^3.2.2",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "^5.3.3"
  }
}
